<?xml version="1.0"?>
<PropertyList>
    <generic>
        <output>
            <line_separator>\n</line_separator>
            <var_separator>,</var_separator>

            <chunk>
                <name>inter-turbine-temp-left</name>
                <type>float</type>
                <format>%03.1f</format>
                <node>/instrumentation/eng-gauge/itt</node>
            </chunk>
            <chunk>
                <name>torque-left</name>
                <type>float</type>
                <format>%03.1f</format>
                <node>/instrumentation/eng-gauge/torque</node>
            </chunk>
            <chunk>
                <name>prop-rpm-left</name>
                <type>float</type>
                <format>%03.1f</format>
                <node>/instrumentation/eng-gauge/prop</node>
            </chunk>
            <chunk>
                <name>percent-rpm-left</name>
                <type>float</type>
                <format>%03.2f</format>
                <node>/instrumentation/eng-gauge/turbine</node>
            </chunk>
            <chunk>
                <name>fuel-flow-pph-left</name>
                <type>float</type>
                <format>%03.2f</format>
                <node>/instrumentation/eng-gauge/fuel-pph</node>
            </chunk>
            <chunk>
                <name>oil-temp-left</name>
                <type>float</type>
                <format>%03.2f</format>
                <node>/engines/engine/oil-temperature-degf</node>
            </chunk>
            <chunk>
                <name>oil-press-left</name>
                <type>float</type>
                <format>%03.3f</format>
                <node>/engines/engine/oil-pressure-psi</node>
            </chunk>

            <chunk>
                <name>inter-turbine-temp-right</name>
                <type>float</type>
                <format>%03.1f</format>
                <node>/instrumentation/eng-gauge/itt[1]</node>
            </chunk>
            <chunk>
                <name>torque-right</name>
                <type>float</type>
                <format>%03.1f</format>
                <node>/instrumentation/eng-gauge/torque[1]</node>
            </chunk>
            <chunk>
                <name>prop-rpm-right</name>
                <type>float</type>
                <format>%03.1f</format>
                <node>/instrumentation/eng-gauge/prop[1]</node>
            </chunk>
            <chunk>
                <name>percent-rpm-right</name>
                <type>float</type>
                <format>%03.2f</format>
                <node>/instrumentation/eng-gauge/turbine[1]</node>
            </chunk>
            <chunk>
                <name>fuel-flow-pph-right</name>
                <type>float</type>
                <format>%03.2f</format>
                <node>/instrumentation/eng-gauge/fuel-pph[1]</node>
            </chunk>
            <chunk>
                <name>oil-temp-right</name>
                <type>float</type>
                <format>%03.2f</format>
                <node>/engines/engine[1]/oil-temperature-degf</node>
            </chunk>
            <chunk>
                <name>oil-press-right</name>
                <type>float</type>
                <format>%03.3f</format>
                <node>/engines/engine[1]/oil-pressure-psi</node>
            </chunk>

			<chunk>
				<name>airspeed-indicated-knots</name>
				<type>float</type>
				<format>%03.3f</format>
				<node>/instrumentation/airspeed-indicator/indicated-speed-kt</node>
			</chunk>
			
            <chunk>
                <name>landing-gear-light-transition</name>
                <type>bool</type>
                <node>/instrumentation/annunciators/gear/in-transition</node>
            </chunk>
            <chunk>
                <name>landing-gear-light-nose</name>
                <type>bool</type>
                <node>/instrumentation/annunciators/gear/nose/down</node>
            </chunk>
            <chunk>
                <name>landing-gear-light-left</name>
                <type>bool</type>
                <node>/instrumentation/annunciators/gear/left/down</node>
            </chunk>
            <chunk>
                <name>landing-gear-light-right</name>
                <type>bool</type>
                <node>/instrumentation/annunciators/gear/right/down</node>
            </chunk>
			<chunk>
				<name>hobbs-running</name>
				<type>bool</type>
				<node>/engines/engine/running</node>
			</chunk>
        </output>
    </generic>
</PropertyList>